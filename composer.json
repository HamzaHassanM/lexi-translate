{
    "name": "omaralalwi/lexi-translate",
    "description": "Laravel translation package with morph relationships and caching.",
    "keywords": [
        "omaralalwi",
        "lexi-translate",
        "laravel",
        "translation",
        "trans",
        "multilanguage",
        "multilingual",
        "languages",
        "many-languages",
        "many-locales",
        "multi-locales",
        "translations",
        "translate",
        "locales",
        "database-translations",
        "laravel-translations",
        "laravel-locales",
        "laravel-multi-language",
        "cache",
        "morph",
        "i18n",
        "localization",
        "text-localization",
        "language-switch",
        "polyglot",
        "dynamic-translation",
        "multi-region",
        "eloquent-translation",
        "translation-management",
        "translation-cache"
    ],
    "homepage": "https://github.com/omaralalwi/lexi-translate",
    "license": "MIT",
    "type": "library",
    "version": "1.0.3",
    "authors": [
        {
            "name": "Omar alalwi",
            "email": "omaralwi2010@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/cache": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Omaralalwi\\LexiTranslate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omaralalwi\\LexiTranslate\\Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omaralalwi\\LexiTranslate\\LexiTranslateServiceProvider"
            ],
            "aliases": {
                "LexiTranslate": "Omaralalwi\\LexiTranslate\\LexiTranslateFacade"
            }
        }
    }
}
